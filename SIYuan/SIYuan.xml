  <Repository>b3log/siyuan</Repository>
  <Registry>https://hub.docker.com/r/b3log/siyuan</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
